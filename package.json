{
  "name": "daoism-dapp",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/daoism-dapp.cjs",
  "module": "./dist/daoism-dapp.mjs",
  "exports": {
    ".": {
      "require": "./dist/daoism-dapp.cjs",
      "import": "./dist/daoism-dapp.mjs"
    }
  },
  "scripts": {
    "ui:dev": "vite --host",
    "ui:build": "tsc && vite build",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --runInBand --coverage",
    "lint:scripts": "eslint . --ext .tsx,.ts,.js,.js",
    "lint:scripts:fix": "eslint . --ext .tsx,.ts,.js,.js --fix",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "yarn format:scripts && yarn format:styles",
    "prepare": "husky install && husky add .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.1.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@ethersproject/contracts": "^5.6",
